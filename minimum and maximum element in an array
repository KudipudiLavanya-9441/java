class Compute 
{
    static pair getMinMax(long a[], long n)  
    {   
        pair p;
        long mn= a[0];
        long mx =-1;
        for (int i = 0; i < a.length; i++) {
        mn = Math.min(a[i], mn);
        mx = Math.max(a[i], mx);
        
    }
     p = new pair(mn,mx);
     return p;
    }
    
}
Input:
N = 6
A[] = {3, 2, 1, 56, 10000, 167}


 output:Output:
min = 1, max =  10000
